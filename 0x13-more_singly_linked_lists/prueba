#include "lists.h"

/**
 * get_nodeint_at_index -
 *
 * Return: 
 */
listint_t *get_nodeint_at_index(listint_t *head, unsigned int index)
{
	unsigned int count;
	listint_t *ptr;

	if (!head)
	{
		return (NULL);
	}
	ptr = head;
	count = 0;
	do
	{
		ptr = ptr->next;
		count++;
	}
	return (ptr);
}
